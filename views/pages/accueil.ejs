<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Accueil</title>
</head>

<header>
</header>

<body>
    <section>
        <p>Welcome tech-watch user !</p>
    </section>
    <section>
        <ul>
            <% newProject.forEach(function(project) { %>
            <li><%= project.subject %></li>
            <li><%= project.date %></li>
            <li>
                <% for(let i=0; i< project.students.length; i++) { %>
                <%= project.students[i].name %>
                <% } %>
            </li>
                <br/>
            <% }); %>
        </ul>
    </section>
    <section>
        <p>List of students they will must do a tech-watch soon:</p>
    </section>
    <section>
        <ul>
            <% newStudent.forEach(function(student) { %>
            <li><%= student.name %></li>
            <% }); %>
        </ul>
    </section>

</body>

</html>